{
  "buildCommand": "NODE_OPTIONS='--max-old-space-size=3584' next build",
  "framework": "nextjs",
  "regions": ["iad1"],
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1",
      "NODE_ENV": "production",
      "SKIP_ENV_VALIDATION": "1"
    }
  },
  "functions": {
    "app/**/*.{js,ts,jsx,tsx}": {
      "maxDuration": 10
    }
  }
}